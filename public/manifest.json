{
    "manifest_version": 2,
    "name": "Facet.ninja",
    "version": "0.0.2",
    "description": "Facet.ninja | Deploy - Learn - Adapt",
    "icons": {
        "512": "facet_ninja_logo.png"
    },
    "browser_action": {
        "default_icon": "facet_ninja_logo.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://unpkg.com; object-src 'self'",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "/static/css/main.css"
            ],
            "js": [
                "/static/js/main.js"
            ]
        }
    ],
    "oauth2": {
        "client_id": "499854537033-htjn2is1b7o0iu0r5bi8rdkgmcjcb2ul.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/userinfo.profile"
        ]
    },
    "key": "",
    "permissions": [
        "<all_urls>",
        "identity",
        "identity.email"
    ]
}